{
  "manifest_version": 2,
  "name": "Boombate url shorter",
  "description": "This extension copy short url from boombate.com",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Нажми, чтобы сократить ссылку!"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "clipboardWrite",
    "*://boombate.com/",
    "*://m.boombate.com/"
  ]
}
